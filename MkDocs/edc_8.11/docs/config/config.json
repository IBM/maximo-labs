{
	"version": "3.0.3",

	/* Speficy protocol type: "RTU" or "TCP" */
	"mode": "TCP",

	/* Serial port settings (for RTU mode only) */
	"port": {
		"name": "COM1",
		"baudRate": 19200,
		"dataBits": 8,
		"parity": "none",
		"stopBits": 1
	},

	/* Frame Delay Override */
	/* the amount of time in ms at the end of the frame (null for default) */
	"frameDelay": null,

	/* TCP port number (for TCP) */
	"tcpPort": 502,

